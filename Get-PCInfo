$omg = {
$a = wmic /node:"localhost" ComputerSystem Get Model
$c = wmic /node:"localhost" ComputerSystem Get Manufacturer
$b = wmic bios get serialnumber
$d = [Environment]::UserName + '@xxxxx.ru'
Send-MailMessage -SmtpServer outlook.pcw.local -To "xxxxx.ru" -Encoding ([System.Text.Encoding]::UTF8) -From "$d" -Subject "Model and Serial Number" -Body "
$c 
$a
$b
"
}
[convert]::ToBase64String([Text.Encoding]::Unicode.GetBytes($omg))

powershell -ExecutionPolicy ByPass -EncodedCommand 
